{
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "curly": "error",
    "eqeqeq": "error",
    "eqnull": "error",
    "immed": "error",
    "latedef": "error",
    "newcap": "error",
    "noarg": "error",
    "undef": "error",
    "strict": "error",
    "trailing": "error",
    "browser": true,
    "node": true,
    "jquery": true,
    "bitwise": false,
    "predef": ["define"]
  }
}
